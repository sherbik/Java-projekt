<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Hyperlink?>
<VBox spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="net.programa.sklep.programsklep.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Login"/>
    <TextField fx:id="login" />
    <Label text="Password"/>
    <TextField fx:id="password"/>
    <Button fx:id="confirm" text="Login" onAction="#Login" alignment="CENTER"/>
    <Hyperlink fx:id="goto_register" onAction="#goto_reg" text="Don't have an account? Register!" alignment="CENTER"/>
</VBox>
